<h1>Listing members</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Last name</th>
    <th>Username</th>
    <th>Email</th>
    <th>Phone</th>
  </tr>

<% for member in @members %>
  <tr>
    <td><%=h member.fname %></td>
    <td><%=h member.lname %></td>
    <td><%=h member.username %></td>
    <td><%=h member.email %></td>
    <td><%=h member.phone %></td>
    <td><%= link_to 'Show', member %></td>
    <td><%= link_to 'Edit', edit_member_path(member) %></td>
    <td><%= link_to 'Delete', member, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New member', new_member_path %>
